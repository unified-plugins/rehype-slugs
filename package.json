{
  "name": "rehype-slugs",
  "version": "0.0.4",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "unist",
    "hast",
    "hast-util",
    "util",
    "utility",
    "html",
    "rehype",
    "slug",
    "slugs",
    "anchor"
  ],
  "repository": "tingyur/rehype-slugs",
  "bugs": "https://github.com/tingyur/rehype-slugs/issues",
  "author": "Zhenyu Zhu <1214327383@qq.com>",
  "types": "types/index.d.ts",
  "files": [
    "types/index.d.ts",
    "lib",
    "index.js"
  ],
  "scripts": {
    "build-bundle": "browserify . -s rehypeSlugs > rehype-slugs.js",
    "build-mangle": "browserify . -s rehypeSlugs -p tinyify > rehype-slugs.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-types": "dtslint types",
    "test-md": "node ./tests/run-md.js",
    "test-html": "node ./tests/run-html.js",
    "debug-md": "node --inspect-brk ./tests/run-md.js",
    "debug-html": "node --inspect-brk ./tests/run-html.js"
  },
  "dependencies": {
    "crypto-random-string": "^3.2.0",
    "hast-util-has-property": "^1.0.4",
    "hast-util-is-element": "^1.0.4",
    "unist-util-is": "^4.0.2",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "vfile-reporter": "^6.0.1",
    "unist-builder": "^2.0.3",
    "unified": "^9.1.0",
    "typescript": "^3.9.7",
    "tinyify": "^3.0.0",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-sanitize": "^3.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "hast-util-sanitize": "^3.0.0",
    "hast": "^1.0.0",
    "dtslint": "^3.6.14",
    "deepmerge": "^4.2.2"
  }
}
